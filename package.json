{
  "name": "@shekhar.raval/rest-lib",
  "version": "1.0.20",
  "description": "Rest Api Library for Node Project Microservices",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "nodemon",
    "tsc": "tsc",
    "pub": "npm version patch && npm run build && npm publish"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "express",
    "logger",
    "microservice",
    "rest-server",
    "error-handling"
  ],
  "author": "RAVAL SHEKHAR",
  "license": "MIT",
  "homepage": "https://github.com/raval-shekhar/rest-lib.git#readme",
  "url": "https://github.com/raval-shekhar/rest-lib.git",
  "email": "raval.shekhar@outlook.com",
  "dependencies": {
    "@shekhar.raval/logger": "^0.0.8",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.3.1",
    "joi": "^17.3.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.20",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
